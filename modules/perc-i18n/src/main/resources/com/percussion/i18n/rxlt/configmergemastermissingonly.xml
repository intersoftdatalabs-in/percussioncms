<?xml version="1.0" encoding="UTF-8"?>


<!--
This configuration file is used to merge the server TMX resource bundle with
that generated from the CMS by the Percussion Language Tool (RXLT). This merging is
required to preserve the existing translations before giving the generated TMX
resource bundle for translation service. This configuration file will generate
only the keys that are present in the bundle generated from CMS and missing in the
master or server resource bundle.
<!DOCTYPE tmxmergeconfig SYSTEM "tmxmergeconfig.dtd">
-->
<tmxmergeconfig>
	<tu>
		<config>
			<!-- Ddelete the tu element if present -->
			<param name="deleteifexists">yes</param>
			<param name="replaceifexists">no</param>
			<param name="modifyifexists">no</param>
			<param name="ignoreifexists">no</param>
			<param name="addifnotexists">no</param>
			<!-- Do nothing if  if not present -->
			<param name="ignoreifnotexists">yes</param>
		</config>
		<note>
			<config>
				<param name="deleteifexists">no</param>
				<!-- tu/note is always overwritten from the server resource bundle -->
				<param name="replaceifexists">yes</param>
				<param name="modifyifexists">no</param>
				<param name="ignoreifexists">no</param>
				<!-- Add the server one if does not exist -->
				<param name="addifnotexists">yes</param>
				<param name="ignoreifnotexists">no</param>
			</config>
		</note>
		<prop>
			<config>
				<param name="deleteifexists">no</param>
				<!-- tu/prop is always overwritten from the server resource bundle -->
				<param name="replaceifexists">yes</param>
				<param name="modifyifexists">no</param>
				<param name="ignoreifexists">no</param>
				<!-- Add the server one if does not exist -->
				<param name="addifnotexists">yes</param>
				<param name="ignoreifnotexists">no</param>
			</config>
		</prop>
		<tuv>
			<config>
				<param name="deleteifexists">no</param>
				<param name="replaceifexists">yes</param>
				<!-- get inside the tuv element -->
				<param name="modifyifexists">no</param>
				<param name="ignoreifexists">no</param>
				<!-- Add the server one if does not exist -->
				<param name="addifnotexists">yes</param>
				<param name="ignoreifnotexists">no</param>
			</config>
			<note>
				<config>
					<param name="deleteifexists">no</param>
					<!-- tuv/note is always overwritten from the server resource bundle -->
					<param name="replaceifexists">yes</param>
					<param name="modifyifexists">no</param>
					<param name="ignoreifexists">no</param>
					<!-- Add the server one if does not exist -->
					<param name="addifnotexists">yes</param>
					<param name="ignoreifnotexists">no</param>
				</config>
			</note>
			<prop>
				<config>
					<param name="deleteifexists">no</param>
					<!-- tuv/prop is always overwritten from the server resource bundle -->
					<param name="replaceifexists">yes</param>
					<param name="modifyifexists">no</param>
					<param name="ignoreifexists">no</param>
					<!-- Add the server one if does not exist -->
					<param name="addifnotexists">yes</param>
					<param name="ignoreifnotexists">no</param>
				</config>
			</prop>
			<seg>
				<config>
					<param name="deleteifexists">no</param>
					<param name="replaceifexists">no</param>
					<!-- get inside the seg element -->
					<param name="modifyifexists">yes</param>
					<param name="ignoreifexists">no</param>
					<!-- Add the server one if does not exist -->
					<param name="addifnotexists">yes</param>
					<param name="ignoreifnotexists">no</param>
				</config>
			</seg>
		</tuv>
	</tu>
</tmxmergeconfig>
