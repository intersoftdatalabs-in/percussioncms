<?xml version="1.0" encoding="UTF-8"?>


<PSXWebdavConfigDef root="//Sites/DemoSite" communityname="default" communityid="10" locale="en-us">


	<PSXWebdavContentType id="305" name="File" contentfield="fileupload" ownerfield="abstractcontent" default="true">
		<PropertyMap>			
			<PSXPropertyFieldNameMapping name="getcontenttype">
				<FieldName>fileupload_type</FieldName>
			</PSXPropertyFieldNameMapping>
			
			<PSXPropertyFieldNameMapping name="getcontentlength">
				<FieldName>fileupload_size</FieldName>
			</PSXPropertyFieldNameMapping>
		</PropertyMap>
	</PSXWebdavContentType>
	

	<PSXWebdavContentType id="306" name="Image" contentfield="imgbody" ownerfield="abstractcontent" default="false">
