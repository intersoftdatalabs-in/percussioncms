  <role rolename="CI_Admin_Members"/>
  <role rolename="CI_Members"/>
  <role rolename="Intranet_Admin_Members"/>
  <role rolename="tomcat"/>
  <role rolename="EI_Admin_Members"/>
  <role rolename="EI_Members"/>
  <role rolename="Default"/>
  <role rolename="manager"/>
  <role rolename="Admin"/>
  <role rolename="admin"/>
  <role rolename="Web_Admin"/>
  <user username="admin1" password="demo" roles="Admin,Default,Web_Admin,EI_Members,EI_Admin_Members"/>
  <user username="tomcat" password="tomcat" roles="admin,manager,tomcat"/>
  <user username="admin2" password="demo" roles="Admin,Default,Web_Admin,CI_Members,CI_Admin_Members"/>
  <user username="rxuser" password="demo" roles="Admin,Default,Web_Admin,EI_Members,EI_Admin_Members,CI_Members,CI_Admin_Members"/>
