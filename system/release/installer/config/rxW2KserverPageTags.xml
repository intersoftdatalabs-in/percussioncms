
 <JspAspTags>
    <tag>
       <opening>&lt;%</opening>
       <closing>%&gt;</closing>
       <disableEscaping>no</disableEscaping>
    </tag>

    <tag>
       <opening>&lt;jsp:</opening>
       <closing>&lt;/jsp:</closing>
       <disableEscaping>no</disableEscaping>
    </tag>

    <tag isXslTag="yes">
       <opening>&lt;xsl:</opening>
       <closing>&gt;</closing>
       <disableEscaping>yes</disableEscaping>
    </tag>

    <tag isXslTag="yes">
       <opening>&lt;!-- begin XSL --&gt;</opening>
       <closing>&lt;!-- end XSL --&gt;</closing>
       <disableEscaping>yes</disableEscaping>
    </tag>

   <!-- added by Microsoft Word when a comment is in the document: remove them entirely -->
    <tag>
       <opening>&lt;![if !supportAnnotations]&gt;</opening>
       <closing>&lt;![endif]&gt;</closing>
       <disableEscaping>yes</disableEscaping>
    </tag>
    <tag>
       <opening>&lt;!--[if supportFields]&gt;</opening>
       <closing>&lt;![endif]--&gt;</closing>
       <disableEscaping>yes</disableEscaping>
    </tag>

   <!-- remove specific Microsoft Word "if" wrappers that contain not well-formed content  -->
   <tag>
      <opening>&lt;!--[if !mso]&gt;</opening>
      <closing>&lt;![endif]--&gt;</closing>
      <disableEscaping>yes</disableEscaping>
   </tag>

   <!-- remove generic Microsoft Word "if" wrappers, but leave the content inside -->
    <tag>
       <opening>&lt;!--[if</opening>
       <closing>&gt;</closing>
       <disableEscaping>yes</disableEscaping>
    </tag>
    <tag>
       <opening>&lt;!-- [if</opening>
       <closing>--&gt;</closing>
       <disableEscaping>yes</disableEscaping>
    </tag>
    <tag>
       <opening>&lt;![if</opening>
       <closing>&gt;</closing>
       <disableEscaping>yes</disableEscaping>
    </tag>
    <tag>
       <opening>&lt;![endif</opening>
       <closing>&gt;</closing>
       <disableEscaping>yes</disableEscaping>
    </tag>

 </JspAspTags>
