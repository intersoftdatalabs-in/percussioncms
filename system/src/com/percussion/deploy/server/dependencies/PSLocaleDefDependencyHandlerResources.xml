<!--
This configuration file is used to merge the source TMX resource bundle with
the master bundle on the target server. This merging is required to preserve the
existing translations.
<!DOCTYPE tmxmergeconfig SYSTEM "tmxmergeconfig.dtd">
-->
<tmxmergeconfig>
   <tu>
      <config>
         <param name="deleteifexists">no</param>
         <param name="replaceifexists">no</param>
         <!-- Get inside the tu element -->
         <param name="modifyifexists">yes</param>
         <param name="ignoreifexists">no</param>
         <!-- Add if not exists for some reason -->
         <param name="addifnotexists">yes</param>
         <param name="ignoreifnotexists">no</param>
      </config>
      <note>
         <config>
            <!-- Translated one overrides the existing one -->
            <param name="replaceifexists">yes</param>
            <param name="ignoreifexists">no</param>
            <param name="deleteifexists">no</param>
            <param name="modifyifexists">no</param>
            <!-- Add if not exists for some reason -->
            <param name="addifnotexists">yes</param>
            <param name="ignoreifnotexists">no</param>
         </config>
      </note>
      <prop>
         <config>
            <param name="deleteifexists">no</param>
            <!-- Translated one overrides the existing one -->
            <param name="replaceifexists">yes</param>
            <param name="modifyifexists">no</param>
            <param name="ignoreifexists">no</param>
            <!-- Add if not exists for some reason -->
            <param name="addifnotexists">yes</param>
            <param name="ignoreifnotexists">no</param>
         </config>
      </prop>
      <tuv>
         <config>
            <param name="deleteifexists">no</param>
            <!-- While tuv is replaced from the translated one. So configuration setting
            for the child elements for tuv have no meaning -->
            <param name="replaceifexists">yes</param>
            <param name="modifyifexists">no</param>
            <param name="ignoreifexists">no</param>
            <!-- Add if not exists for some reason -->
            <param name="addifnotexists">yes</param>
            <param name="ignoreifnotexists">no</param>
         </config>
         <!-- these seetings are ignored -->
         <note>
            <config>
               <param name="replaceifexists">yes</param>
               <param name="ignoreifexists">no</param>
               <param name="deleteifexists">no</param>
               <param name="modifyifexists">no</param>
               <param name="addifnotexists">yes</param>
               <param name="ignoreifnotexists">no</param>
            </config>
         </note>
         <!-- these seetings are ignored -->
         <prop>
            <config>
               <param name="replaceifexists">no</param>
               <param name="ignoreifexists">no</param>
               <param name="deleteifexists">no</param>
               <param name="modifyifexists">yes</param>
               <param name="addifnotexists">yes</param>
               <param name="ignoreifnotexists">no</param>
            </config>
         </prop>
         <!-- these seetings are ignored -->
         <seg>
            <config>
               <param name="replaceifexists">yes</param>
               <param name="ignoreifexists">no</param>
               <param name="deleteifexists">no</param>
               <param name="modifyifexists">no</param>
               <param name="addifnotexists">yes</param>
               <param name="ignoreifnotexists">no</param>
            </config>
         </seg>
      </tuv>
   </tu>
</tmxmergeconfig>

