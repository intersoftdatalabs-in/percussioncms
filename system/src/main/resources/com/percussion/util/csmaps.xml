	<PSEncoding stdName="ISO-8859-7" javaName="ISO8859_7" desc="">
	<!--
		Note that Windows-1253 differs from 8859-7 in its placement of the capital
		alpha with tonos and some other symbols. Therefore, it is listed as
		a separate type.
    -->
		<Alias name="8859_7"/>
		<Alias name="CSISOLATINGREEK"/>
		<Alias name="ECMA-118"/>
		<Alias name="ELOT_928"/>
		<Alias name="GREEK8"/>
