	<PSEncoding stdName="EUC-KR" javaName="EUC_KR" desc="Extended Unix Code, packed for Korean">
		<Alias name="CSEUCKR"/>

		<!--
			Note that both Java and IBM's XML4J XML parser list EUC-KR as equivalent
			to KSC5601. Don't know if that's correct, because IANA and RFC1700 list
			them as separate. Here are the KSC5601 equivalences.
		-->
		<Alias name="KS_C_5601-1987"/>
		<Alias name="ISO-IR-149"/>
		<Alias name="KS_C_5601-1989"/>
		<Alias name="KSC_5601"/>
		<Alias name="KS_C_5601"/>
		<Alias name="KOREAN"/>
		<Alias name="CSKSC56011987"/>
	</PSEncoding>

	<PSEncoding stdName="ISO-2022-KR" javaName="ISO2022KR" desc="Korean">
		<Alias name="ISO2022KR"/>
		<Alias name="CSISO2022KR"/>
		<Alias name="KR2022"/>
	</PSEncoding>

