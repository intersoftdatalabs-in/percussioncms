<?xml version="1.0" encoding="UTF-8" ?>


<rxftconfig>
<!-- list of server aliases to connect to different servers and urls -->
    <url serveralias="sample1" host="localhost" port="9992">/Rhythmyx/rxfiletracker/contentlist.xml</url>
    <url serveralias="sample2" host="127.0.0.1" port="9992">/Rhythmyx/rxfiletracker/contentlist.xml</url>
<!-- list of options that can be set in the application -->
    <purgelocalcopyafterupload>no</purgelocalcopyafterupload>
    <promptbeforepurge>yes</promptbeforepurge>
    <promptbeforeoverwrite>yes</promptbeforeoverwrite>
<!-- this element stores the last connected server alias and user -->
    <current serveralias="sample1" userid="demouser"/>
 </rxftconfig>
