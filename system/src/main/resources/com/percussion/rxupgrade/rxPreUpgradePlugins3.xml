<?xml version="1.0" encoding="UTF-8"?>


<rxupgrade>
    <!-- Initializes the spring services to be used by upgrade plugins -->
	<module name="server" logfile="upgradeSpringInit.log">
		<from major="1" minor="0" micro="-1" build="-1"/>
		<to major="-1" minor="-1" micro="-1" build="-1"/>
		<plugins>
		  <plugin name="PSSpringUpgradePluginInit">
		     <class>com.percussion.install.PSSpringUpgradePluginInit</class>
			<data/>
		  </plugin>
		</plugins>
	</module>
</rxupgrade>
	
