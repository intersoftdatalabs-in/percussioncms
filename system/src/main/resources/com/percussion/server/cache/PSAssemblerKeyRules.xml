<!--
  Defines rules for optional assembler cache key inclusions.  See 
  com.percussion.server.cache.PSKeyRules for more information.
-->
<PSXKeyRules>
   <KeyRule keyName="sessionid">
      <PSXRule boolean="and">
         <PSXExtensionCallSet id="0">
            <PSXExtensionCall id="1">
               <name>Java/global/percussion/contentassembler/sys_casCacheBySessionRule</name>
            </PSXExtensionCall>
         </PSXExtensionCallSet>
      </PSXRule>
   </KeyRule>
</PSXKeyRules>
