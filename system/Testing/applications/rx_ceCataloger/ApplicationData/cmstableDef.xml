				<size/>
				<allowsnull>no</allowsnull>
			</column>
			<column name="REVISIONID">
				<jdbctype>INTEGER</jdbctype>
				<size/>
				<allowsnull>no</allowsnull>
			</column>
			<column name="DISPLAYTITLE">
				<jdbctype>VARCHAR</jdbctype>
				<size>50</size>
