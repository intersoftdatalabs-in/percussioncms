<?xml version='1.0' encoding='UTF-8'?>
<!ELEMENT ContentStatus (WorkflowName, StateName, ContentTypeName, DataVariant, Title, CheckOutUserName, LastModifier, LastModified, CreatedBy, CreatedDate, StartDate?, ExpirationDate?, AgingTime?, PublishDate?, PathName?, Suffix?, Lock, CommunityId )>
<!ELEMENT CommunityId (#PCDATA)>
<!ELEMENT Lock (#PCDATA)>
<!ELEMENT Suffix (#PCDATA)>
<!ELEMENT PathName (#PCDATA)>
<!ELEMENT PublishDate (#PCDATA)>
<!ELEMENT AgingTime (#PCDATA)>
<!ELEMENT ExpirationDate (#PCDATA)>
<!ELEMENT StartDate (#PCDATA)>
<!ELEMENT CreatedDate (#PCDATA)>
<!ELEMENT CreatedBy (#PCDATA)>
<!ELEMENT LastModified (#PCDATA)>
<!ELEMENT LastModifier (#PCDATA)>
<!ELEMENT CheckOutUserName (#PCDATA)>
